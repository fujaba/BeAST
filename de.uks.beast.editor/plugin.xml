<?xml version="1.0" encoding="UTF-8"?>
<plugin>

<!-- old type: hardwareComposition -->
<!-- old wizard: HCDWizard.java -->
<!-- old wizardPage: HCDWizardPage.java -->

	<!-- Execute BeAST test...-->
	<extension point="org.eclipse.ui.popupMenus"> 
	    <objectContribution
	      adaptable="true"
	      id="de.uks.beast.editor.popup"
	      nameFilter="*.diagram"
	      objectClass="org.eclipse.core.resources.IFile">
	      <action
	         id="de.uks.beast.editor.beastAction" 
	         label="Execute BeAST test..."
	         menubarPath="additions"
	         class="de.uks.beast.editor.action.BeASTAction"> 
	      </action>
	    </objectContribution>
	</extension>

	<!-- Diagram type -->
	<extension
         point="org.eclipse.graphiti.ui.diagramTypes">
		<diagramType name="Editor Diagram Type" type="beast" id="de.uks.beast.editor.editorDiagramType">
		</diagramType>
	</extension>
	
	<!-- Diagram provider -->
	<extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
		<diagramTypeProvider name="Editor Diagram Type Provider" class="de.uks.beast.editor.provider.EditorDiagramTypeProvider"
			id="de.uks.beast.editor.editorDiagramTypeProvider">
			<diagramType
               id="de.uks.beast.editor.editorDiagramType">
			</diagramType>
			<imageProvider
           	 	id="de.uks.beast.editor.provider.EditorImageProvider">
      		</imageProvider>
		</diagramTypeProvider>
	</extension>
	
	<extension
      	point="org.eclipse.graphiti.ui.imageProviders">
   		<imageProvider
         	class="de.uks.beast.editor.provider.EditorImageProvider"
         	id="de.uks.beast.editor.provider.EditorImageProvider">
   		</imageProvider>
	</extension> 
	
	
	<!-- PropertyContributor -->
	<extension 
		point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
		<propertyContributor contributorId="beast.PropertyContributor">
			<propertyCategory category="Configuration">
			</propertyCategory>
		</propertyContributor>
	</extension>
	
	
	
	<!-- PropertyTabs -->
	<extension
      point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
		<propertyTabs
          contributorId="beast.PropertyContributor">
			<propertyTab category="Configuration" id="graphiti.main.tab.server" label="Config">
			</propertyTab>
		</propertyTabs>
	</extension>
	<extension
      point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
		<propertyTabs
          contributorId="beast.PropertyContributor">
			<propertyTab category="Configuration" id="graphiti.main.tab.network" label="Config">
			</propertyTab>
		</propertyTabs>
	</extension>
	<extension
      point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
		<propertyTabs
          contributorId="beast.PropertyContributor">
			<propertyTab category="Configuration" id="graphiti.main.tab.router" label="Config">
			</propertyTab>
		</propertyTabs>
	</extension>
	<extension
      point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
		<propertyTabs
          contributorId="beast.PropertyContributor">
			<propertyTab category="Configuration" id="graphiti.main.tab.service" label="Jobs">
			</propertyTab>
		</propertyTabs>
	</extension>
	
	
	<!-- PropertySections -->
	<extension 
		point="org.eclipse.ui.views.properties.tabbed.propertySections">
		<propertySections
          contributorId="beast.PropertyContributor">
			<propertySection class="de.uks.beast.editor.property.section.sections.ServerPropertySection"
				filter="de.uks.beast.editor.property.filter.ServerPropertyFilter"
				id="graphiti.main.tab.server" tab="graphiti.main.tab.server">
			</propertySection>
		</propertySections>
	</extension>
	<extension 
		point="org.eclipse.ui.views.properties.tabbed.propertySections">
		<propertySections
          contributorId="beast.PropertyContributor">
			<propertySection class="de.uks.beast.editor.property.section.sections.RouterPropertySection"
				filter="de.uks.beast.editor.property.filter.RouterPropertyFilter"
				id="graphiti.main.tab.router" tab="graphiti.main.tab.router">
			</propertySection>
		</propertySections>
	</extension>
	<extension 
		point="org.eclipse.ui.views.properties.tabbed.propertySections">
		<propertySections
          contributorId="beast.PropertyContributor">
			<propertySection class="de.uks.beast.editor.property.section.sections.NetworkPropertySection"
				filter="de.uks.beast.editor.property.filter.NetworkPropertyFilter"
				id="graphiti.main.tab.network" tab="graphiti.main.tab.network">
			</propertySection>
		</propertySections>
	</extension>
	<extension 
		point="org.eclipse.ui.views.properties.tabbed.propertySections">
		<propertySections
          contributorId="beast.PropertyContributor">
			<propertySection class="de.uks.beast.editor.property.section.sections.HadoopPropertySection"
				filter="de.uks.beast.editor.property.filter.HadoopPropertyFilter"
				id="graphiti.main.tab.service" tab="graphiti.main.tab.service">
			</propertySection>
		</propertySections>
	</extension>
 
 <!-- Wizard -->
 <extension
       point="org.eclipse.ui.newWizards">
    <category
          id="de.uks.beast.editor"
          name="BeAST Test">
    </category>
    <wizard
          category="de.uks.beast.editor"
          class="de.uks.beast.editor.wizard.BeastWizard"
          icon="icons/sample.gif"
          id="de.uks.beast.editor.wizard.BeastWizard"
          name="Beast Diagram">
    </wizard>
 </extension>
  
</plugin>
